                  placeholder="Search"
                  ng-init="fieldCount= 'patientsex'"
                />
                <span class="input-group-btn">
                  <button
                    type="button"
                    class="btn btn-primary"
                    ng-click="renderCharts()"
                  >
                    Render Charts
                  </button>
                </span>
                <span class="input-group-btn">
                  <button
                    type="button"
                    class="btn btn-primary"
