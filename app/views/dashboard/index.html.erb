<div class="container-fluid" ng-controller="DashboardCtrl">
  <div class="row">
    <div class="col-sm-3 col-md-2 sidebar" style="overflow: visible !important">
      <accordion close-others="oneAtATime">
        <accordion-group
          ng-repeat="item in (safetyReportProperties | filter:{value: {visible: true}})"
          ng-init="status.isOpen=true"
          is-open="status.isOpen"
        >
          <accordion-heading>
            {{item.field}}
            <i class="pull-right glyphicon" ng-class="{'glyphicon-chevron-down': !status.isOpen, 'glyphicon-chevron-up': status.isOpen}"></i>
          </accordion-heading>

            <select
              ng-if="item.value.elType == 'multi_select'"
              chosen
              multiple
              data-placeholder="Choose"
              ng-model="tempSearchOptions[item.full_field]"
              ng-options="(item.value.trackByIndex ? key : value) as  value for (key,value) in item.value.store"
            >
            </select>

            <select
              ng-if="item.value.elType == 'select'"
              chosen
              data-placeholder="Choose"
              ng-model="tempSearchOptions[item.full_field]"
              ng-options="(item.value.trackByIndex ? key : value) as  value for (key,value) in item.value.store"
            >
            </select>

            <div
              ng-if="item.value.elType == 'range'"
              range-slider
              ng-model="tempSearchOptions[item.full_field]"
              min="item.value.min"
              max="item.value.max"
              model-min="tempSearchOptions[item.full_field].min"
              model-max="tempSearchOptions[item.full_field].max"
              ng-init="tempSearchOptions[item.full_field].min=item.value.min; tempSearchOptions[item.full_field].max=item.value.max"
            ></div>



            <span
              ng-if="item.value.elType == 'date'"
            >

              <p class="input-group">
                <input
                  type="text"
                  class="form-control"
                  datepicker-popup="{{format}}"
                  ng-model="tempSearchOptions[item.full_field].from"
                  min-date="minDate"
                  datepicker-options="dateOptions"
                  ng-required="false"
                  close-text="Close"
                  placeholder="From"
                />
                <span class="input-group-btn">
                  <button type="button" class="btn btn-default" ng-click="open($event)"> <i class="glyphicon glyphicon-calendar"></i>
                  </button>
                </span>
              </p>

              <p class="input-group">
                <input
                  type="text"
                  class="form-control"
                  datepicker-popup="{{format}}"
                  ng-model="tempSearchOptions[item.full_field].to"
                  min-date="minDate"
                  datepicker-options="dateOptions"
                  ng-required="false"
                  close-text="Close"
                  placeholder="To"
                />
                <span class="input-group-btn">
                  <button type="button" class="btn btn-default" ng-click="open($event)"> <i class="glyphicon glyphicon-calendar"></i>
                  </button>
                </span>
              </p>


            </span>


          </div>

          This is just some content to illustrate fancy headings.
        </accordion-group>
      </accordion>


      <ul class="nav nav-sidebar">
        <li
          ng-init="isCollapsed=false"
          ng-repeat="item in (safetyReportProperties | filter:{value: {visible: true}})"
          ng-click="isCollapsed = true"
        >
          <div collapse="!!isCollapsed">
            <a href="#">{{item.field}} {{item.full_field}}</a>
          </div>

        </li>

      </ul>
      <ul class="nav nav-sidebar">
        <li class="active"><a href="#">Overview <span class="sr-only">(current)</span></a></li>
