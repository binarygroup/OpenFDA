          <div class="row">
            <script type="text/ng-template" id="json_renderer.html">
              <li
                ng-init="current_key = (parent_key != null ? (parent_key + '.' + key) : key); temp = temp || {}"
              >
                <label ng-if="value.visible">{{key}}</label>
                <span
                  ng-if="value.visible && !value.children"

                >
                  <input
                    type="text"
                    ng-model="searchOptions[current_key]"
                    ng-required="false"
                  >

                  <input
                    ng-if="isText(value)"
                    type="text"
                    ng-model="temp[current_key]"
                    ng-required="false"
                    ng-blur="value.process($event, this, searchOptions)"
                  >

                  <span
                    ng-if="isDate(value)"
                  >

                    <p class="input-group">
                      <input
                        type="text"
                        class="form-control"
                        datepicker-popup="{{format}}"
                        ng-model="temp.from"
                        min-date="minDate"
                        datepicker-options="dateOptions"
                        ng-required="false"
                        close-text="Close"
                        placeholder="From"
                        ng-blur="value.process($event, this, searchOptions)"
                      />
                      <span class="input-group-btn">
                        <button type="button" class="btn btn-default" ng-click="open($event)"> <i class="glyphicon glyphicon-calendar"></i>
                        </button>
                      </span>
                    </p>

                    <p class="input-group">
                      <input
                        type="text"
                        class="form-control"
                        datepicker-popup="{{format}}"
                        ng-model="temp.to"
                        min-date="minDate"
                        datepicker-options="dateOptions"
                        ng-required="false"
                        close-text="Close"
                        placeholder="To"
                        ng-blur="value.process($event, this, searchOptions)"
                      />
                      <span class="input-group-btn">
                        <button type="button" class="btn btn-default" ng-click="open($event)"> <i class="glyphicon glyphicon-calendar"></i>
                        </button>
                      </span>
                    </p>


                  </span>


                </span>

              <ul
                  class="list-group"
                  ng-if="value.children"
                  ng-init="parent_key = current_key"
                >
                  <li
                    ng-repeat="(key, value) in value.children"
                    ng-include= "'json_renderer.html'"
                  ></li>
                </ul>
              </li>
            </script>

            <ul class="list-group">
              <li
                ng-repeat="(key, value) in safetyReportObject | orderBy:'order':true | orderBy:'inTable' "
                data-ng-include = "'json_renderer.html'"
              >
            </li><ul>

          </div>

        </form>
      </section>

      <div class="row placeholders">
        <div class="col-xs-6 col-sm-6 placeholder">
          <h4>{{stats.total}}</h4>
          <span class="text-muted">Total</span>
        </div>
        <div class="col-xs-6 col-sm-6 placeholder">
          <!-- <img data-src="holder.js/200x200/auto/vine" class="img-responsive" alt="Generic placeholder thumbnail"> -->
          <h4>Label</h4>
          <span class="text-muted">Something else</span>
        </div>
      </div>

      <h2 class="sub-header">Search Results</h2>
      <div
        id="search_results"
        ui-grid="gridOptions"
        ui-grid-infinite-scroll
        class="grid"
      ></div>
